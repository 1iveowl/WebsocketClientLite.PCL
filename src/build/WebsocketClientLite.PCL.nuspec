        <dependency id="System.Net.NetworkInformation" version="4.3.0" />
      </group>

      <group targetFramework="netstandard2.0">
        <dependency id="System.Reactive" version="3.1.1"/>
        <dependency id="SocketLite.PCL" version="4.1.5"/>
        <dependency id="HttpMachine.PCL" version="3.1.0"/>
      </group>
      

    </dependencies>
    
  </metadata>
    <files>
      <file src="..\main\WebsocketClientLite.Netstandard\bin\Release\netstandard1.2\WebsocketClientLite.*" target="lib\netstandard1.2" />
